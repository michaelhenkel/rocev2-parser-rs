    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rocev2-parser-rs'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rocev2-parser-rs",
                    "--package=rocev2-parser-rs"
                ],
                "filter": {
                    "name": "rocev2-parser-rs",
                    "kind": "bin"
                }
            },
            "args": [
                "-f",
                "../rdma.pcap"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}